<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.ChatController">
    <ListView fx:id="listViewClient" layoutX="15.0" layoutY="61.0" prefHeight="500.0" prefWidth="360.0" />
    <TextField fx:id="textFieldClient" layoutX="15.0" layoutY="567.0" onAction="#sendToServer" prefHeight="25.0" prefWidth="360.0" />
   <Label layoutX="181.0" layoutY="27.0" text="CLIENT" />
   <ListView fx:id="listViewServer" layoutX="426.0" layoutY="61.0" prefHeight="500.0" prefWidth="360.0" />
   <TextField fx:id="textFieldServer" layoutX="426.0" layoutY="567.0" onAction="#sendToClient" prefHeight="25.0" prefWidth="360.0" />
   <Button fx:id="btnMoveToServer" layoutX="385.0" layoutY="275.0" mnemonicParsing="false" onAction="#moveToServer" text="-&gt;" />
   <Button fx:id="btnMoveToClient" layoutX="385.0" layoutY="303.0" mnemonicParsing="false" onAction="#moveToClient" text="&lt;-" />
   <Label layoutX="587.0" layoutY="27.0" text="SERVER" />
</AnchorPane>
